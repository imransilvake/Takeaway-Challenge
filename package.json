{
    "name": "takeaway-challenge",
    "version": "0.0.2",
    "private": true,
    "scripts": {
        "start": "REACT_APP_ENV=development react-scripts start",
        "build": "sh -ac './src/environment/ .env.${REACT_APP_ENV}; react-scripts build'",
        "build:development": "REACT_APP_ENV=development npm run build",
        "build:next": "REACT_APP_ENV=next npm run build",
        "build:production": "REACT_APP_ENV=production npm run build",
        "serve": "serve -s build",
        "lint:scss": "stylelint '**/*.scss'; exit 0",
        "lint:es": "eslint --ext .jsx --ext .js src/",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "dependencies": {
        "@imransilvake/scss-framework": "2.0.7",
        "@material-ui/core": "latest",
        "@material-ui/icons": "3.0.2",
        "classnames": "2.2.6",
        "firebase": "10.9.0",
        "i18next": "14.1.1",
        "i18next-browser-languagedetector": "2.2.4",
        "md5": "2.2.1",
        "moment": "2.24.0",
        "prop-types": "latest",
        "react": "16.8.1",
        "react-dom": "16.8.1",
        "react-i18next": "10.0.3",
        "react-redux": "6.0.0",
        "react-redux-firebase": "2.2.6",
        "react-router-dom": "4.3.1",
        "react-scripts": "2.1.3",
        "redux": "4.0.1",
        "redux-thunk": "2.3.0",
        "redux-devtools-extension": "2.13.8"
    },
    "devDependencies": {
        "eslint-config-airbnb": "17.1.0",
        "eslint-plugin-react": "7.12.4",
        "node-sass": "4.14.1",
        "stylelint": "9.10.1",
        "stylelint-order": "2.0.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
